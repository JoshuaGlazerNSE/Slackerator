{
  "name": "slackerator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "async": "^1.4.2",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "request": "^2.60.0",
    "serve-favicon": "~2.3.0"
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "quotes": 0,
      "no-trailing-spaces": 0,
      "no-multi-spaces": 0,
      "space-infix-ops": 0,
      "space-unary-ops": 0,
      "semi-spacing": 0,
      "comma-spacing": 0,
      "no-underscore-dangle": 0,
      "key-spacing": 0,
      "eol-last": 0,
      "comma-dangle": 0,
      "no-mixed-spaces-and-tabs": 0,
      "no-process-exit": 0
    }
  }
}
